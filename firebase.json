{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "/Timer/**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache "
          }
        ]
      }
    ],
    "redirects": [
      {
        "source": "/timer",
        "destination": "/Timer",
        "type": 301
      }
    ]
  }
}